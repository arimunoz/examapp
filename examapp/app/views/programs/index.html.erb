<p id="notice"><%= notice %></p>

<h1>Programs</h1>
<p>Here our the links for your convenience. </p>
<%= link_to 'Faculties', faculties_path %>
<%= link_to 'Enrollments', enrollments_path %>
<%= link_to 'Courses', courses_path %>
<%= link_to 'Home', root_path %>
<%= link_to "Sign Out", destroy_user_session_path, :method => :delete %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Budget</th>
      <th>Start date</th>
      <th>Faculty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.name %></td>
        <td><%= program.budget %></td>
        <td><%= program.start_date %></td>
        <td><%= program.faculty_id %></td>
        <td><%= link_to 'Show', program %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

