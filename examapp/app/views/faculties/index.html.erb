<p id="notice"><%= notice %></p>

<h1>Faculties</h1>
<p>Here our the links for your convenience. </p>
<%= link_to 'Enrollments', enrollments_path %>
<%= link_to 'Programs', programs_path %>
<%= link_to 'Courses', courses_path %>
<%= link_to 'Home', root_path %>
<%= link_to "Sign Out", destroy_user_session_path, :method => :delete %>
<table>
  <thead>
    <tr>
      <th>Last name</th>
      <th>First name</th>
      <th>Hire date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @faculties.each do |faculty| %>
      <tr>
        <td><%= faculty.last_name %></td>
        <td><%= faculty.first_name %></td>
        <td><%= faculty.hire_date %></td>
        <td><%= link_to 'Show', faculty %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

